{
  "name": "brandibble-redux",
  "version": "1.1.1",
  "description": "A Redux Framework for Brandibble.co",
  "repository": "https://github.com/sanctuarycomputer/brandibble-redux",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4.3.0"
  },
  "main": "dist/brandibble-redux.min.js",
  "scripts": {
    "lint": "eslint src",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/brandibble-redux.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/brandibble-redux.min.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run build",
    "test": "node ./node_modules/karma/bin/karma start"
  },
  "author": "Sanctuary Computer <hello@sanctuary.computer> (http://sanctuary.computer/)",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-preset-react-native": "^1.9.1",
    "brandibble": "~1.0.1",
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "localforage": "^1.4.3",
    "lodash.find": "^4.6.0",
    "lodash.map": "^4.6.0",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "redux": "^3.6.0",
    "redux-crud": "^3.0.1",
    "redux-mock-store": "^1.2.2",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.2.0"
  },
  "dependencies": {}
}
