{
  "name": "brandibble-redux",
  "version": "1.13.8",
  "description": "A Redux Framework for Brandibble.co",
  "repository": "https://github.com/sanctuarycomputer/brandibble-redux",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4.3.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/brandibble-redux.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/brandibble-redux.min.js --config webpack.config.production.js",
    "build": "npm run build:lib",
    "prepare": "npm run build",
    "test": "node ./node_modules/karma/bin/karma start",
    "format": "prettier-eslint \"src/**/*.js\" --write"
  },
  "author": "Sanctuary Computer <hello@sanctuary.computer> (http://sanctuary.computer/)",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "brandibble": "git+https://git@github.com/sanctuarycomputer/brandibble.js",
    "localforage": "^1.5.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.map": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "moment": "^2.18.1",
    "redux": "^3.7.2",
    "redux-promise-middleware": "4.2.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^1.9.1",
    "chai": "^3.5.0",
    "dotenv": "^6.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.4.2",
    "prettier-eslint-cli": "^4.7.1",
    "redux-mock-store": "^1.2.3",
    "webpack": "^3.3.0"
  }
}
